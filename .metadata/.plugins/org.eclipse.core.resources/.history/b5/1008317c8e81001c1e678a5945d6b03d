import java.util.Scanner;

public class ArrayTest {

	public static void main(String[] args) {
		
		System.out.println("Enter values : ");
		int a[100];
		for(int i=0;i<5;i++) {
			Scanner s=new Scanner(System.in); 
			int a[i]=s.nextInt();
		}
		for(int i=0;i<5;i++) {
			System.out.println(""+a[i]); 
		}
	}
}
