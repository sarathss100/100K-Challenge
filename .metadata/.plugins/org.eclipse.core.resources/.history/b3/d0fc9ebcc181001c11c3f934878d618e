#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int limit,i,a[100],j,temp;
	setbuf(stdout,NULL);
	printf("Please enter array limit : ");
	scanf("%d",&limit);
	printf("Please enter values : ");
	for(i=0;i<limit;i++){
		scanf("%d",&a[i]);
	}
	for(i=0;i<limit;i++){
		for(j=i+1;j<limit;j++){
			if(a[i]>a[j]){
				temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
	}
	for(i=0;i<limit;i++){
			printf("%d\t",a[i]);
		}
	return EXIT_SUCCESS;
}
