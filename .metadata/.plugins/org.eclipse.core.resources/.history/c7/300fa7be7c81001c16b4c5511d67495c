import java.util.Scanner;

public class Pattern {

	public static void main(String[] args) {
		
		System.out.println("Enter limit : ");
		
		Scanner s=new Scanner(System.in);
		int i,j,limit=6;
		
		for(i=0;i<limit;i++) {
			for(j=0;j<=i;j++) {
				System.out.println("*");
			}
			System.out.println();
		}
	}
}
